#
# Example form file -- goes with axlform.il
#
FILE_TYPE=FORM_DEFN VERSION=2
FORM AUTOGREYTEXT 
FIXED
PORT 20 5
HEADER "Form Field Type Demo"

TILE

GROUP "Align Directions"
FLOC 2 2
FSIZE 14 6
ENDGROUP

FIELD radioVertical
FLOC 4 4
CHECKLIST "Vertical" "rg"
ENDFIELD

FIELD radioHorizontal
FLOC 4 6
CHECKLIST "Horizontal" "rg"
ENDFIELD

FIELD buttonNext
FLOC 2 8
MENUBUTTON "next" 10 3
ENDFIELD

FIELD buttonDone
FLOC 2 10
MENUBUTTON "Ok" 10 3
ENDFIELD

FLEXMODE edgegravity
FLEX tab 0 0 1 1
FLEX listfield 0 0 1 1
FLEX tree 0 0 1 1
FLEX progress 0 0 1 0
FLEX enum 0 0 1 0
FLEX string 0 0 1 0
FLEX strwenum 0 0 1 0
FLEX float 0 0 1 0
FLEX int 0 0 1 0
FLEX t1 0 0 0 0
FLEX g2 0 1 0 0
FLEX bmpcheck 1 0 0 0
FLEX bmp1 0 0 1 1
FLEX stc 0.5 0 0 0
FLEX color 0.0 0 0.5 0
FLEX icolor 0.5 0 0.5 0
FLEX slidebar 0 0 1 0

ENDTILE

ENDFORM
